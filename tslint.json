{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "interface-name": false,
    "interface-over-type-literal": false,
    "member-ordering": [true, { "order": "fields-first" }],
    "no-submodule-imports": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "prettier": true
  }
}
