    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "reformat": "run-s reformat:src reformat:test reformat:scripts",
    "reformat:src": "prettier --write app/*.js app/**/*.js",
    "reformat:test": "prettier --write test/*.js test/**/*.js",
