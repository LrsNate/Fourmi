{
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "electron": "^1.4.15",
    "electron-builder": "^11.5.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-angular": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "json-loader": "^0.5.4",
    "pre-commit": "^1.2.2",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "postinstall": "cd app && npm install",
    "start": "cross-env NODE_ENV=development electron app",
    "lint": "eslint 'app/**/*.js'",
    "webpack": "webpack --progress --colors --display-modules --config webpack.prod.config.js",
    "webpack-watch": "webpack --progress --colors --watch",
    "pack": "build --dir",
    "predist": "npm run webpack",
    "dist": "build"
  },
  "pre-commit": [
    "lint"
  ],
  "build": {
    "appId": "fr.nate.martial",
    "category": "public.app-category.education"
  }
}
